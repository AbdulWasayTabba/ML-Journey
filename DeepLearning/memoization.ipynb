    "# fibianici\n",
    "\n",
    "def fib(n,d):\n",
    "    if n in d:\n",
    "        return d[n]\n",
    "    else:\n",
    "        d[n]=  fib(n-1,d)+fib(n-2,d)\n",
    "        return d[n]"
   ]
